	<query language="SQL"><![CDATA[SELECT 
    staff.staff_id AS vendedor_id,
    staff.first_name AS vendedor_nombre,
    staff.last_name AS vendedor_apellidos,
    customer.first_name AS cliente_nombre,
    customer.last_name AS cliente_apellidos,
    customer.email AS cliente_email,
    address.phone AS cliente_telefono
FROM sakila.customer
JOIN sakila.store ON customer.store_id = store.store_id
JOIN sakila.staff ON store.manager_staff_id = staff.staff_id
JOIN sakila.address ON customer.address_id = address.address_id
WHERE customer.active = 0
ORDER BY staff.staff_id, customer.last_name, customer.first_name;
]]></query>
