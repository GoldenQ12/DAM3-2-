	<query language="SQL"><![CDATA[SELECT 
    city.city AS poblacion,  
    customer.first_name AS nombre,  
    customer.last_name AS apellidos,  
    customer.create_date AS fecha_alta
FROM sakila.customer
JOIN sakila.address ON customer.address_id = address.address_id
JOIN sakila.city ON address.city_id = city.city_id
ORDER BY city.city, customer.last_name, customer.first_name;
]]></query>
